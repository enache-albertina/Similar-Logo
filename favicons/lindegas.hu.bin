				$("#suggest_Result_div").css("display", "none");
			}
			if($(window).width() <=400){ 
			$(window).off('scroll');
			}; 
		}
	});	
	if($("#isEnableUnifyDesign").length>0){
	$(document).click(function() {
		$("#SuggestDiv").empty();

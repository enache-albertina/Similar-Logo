            elqImg.setAttribute('height', '1px');
            elqImg.setAttribute('width', '1px');
            elqImg.setAttribute('style', 'display:none; border: 0');

            document.addEventListener('DOMContentLoaded', function() {
                document.body.appendChild(elqImg);
            });
        }
    });
